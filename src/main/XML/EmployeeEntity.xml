<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="main.jpa.entity.EmployeeEntity">
        <table name="Employee" schema="dbo" catalog="CustomDB"/>
        <attributes>
            <id name="empId">
                <column name="EmpID" precision="-1"/>
            </id>
            <basic name="firstName">
                <column name="FirstName"/>
            </basic>
            <basic name="lastName">
                <column name="LastName"/>
            </basic>
            <basic name="city">
                <column name="City"/>
            </basic>
            <basic name="street">
                <column name="Street" nullable="false"/>
            </basic>
            <basic name="salary">
                <column name="Salary" precision="-1"/>
            </basic>
            <basic name="loginId">
                <column name="LoginID" nullable="false"/>
            </basic>
            <basic name="loginPass">
                <column name="LoginPass" nullable="false"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
