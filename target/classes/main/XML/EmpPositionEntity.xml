<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="main.jpa.entity.EmpPositionEntity">
        <table name="EmpPosition" schema="dbo" catalog="CustomDB"/>
        <attributes>
            <id name="positionCode">
                <column name="PositionCode" length="1"/>
            </id>
            <basic name="position">
                <column name="Position"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
